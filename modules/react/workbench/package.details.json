{
  "template":    "react",
  "name":        "@fusionconfig/workbench",
  "description": "Fusion Workbench. Gui for working with transformations and schemas",
  "packageJson": {
    "dependencies":    {
      "@laoban/utils":                 "${properties.laobanVersion}",
      "@itsmworkbench/browserurlstore": "${properties.itsmVersion}",
      "@itsmworkbench/events":          "${properties.itsmVersion}"
    },
    "devDependencies": {},
    "bin":             {
      "fusion": "dist/index.js"
    }
  },
  "links":       [
    "@itsmworkbench/dependentdata",
    "@fusionconfig/config",
    "@fusionconfig/jsonata",
    "@fusionconfig/react_components",
    "@fusionconfig/react_explorer",
    "@fusionconfig/react_editor",
    "@fusionconfig/react_routing",
    "@fusionconfig/utils",
    "@fusionconfig/tests",
    "@fusionconfig/browsertests",
    "@fusionconfig/i18n"
  ],
  "guards":      {
    "compile": true,
    "publish": true,
    "test":    false
  }
}
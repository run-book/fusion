{
  "template":    "typescript",
  "name":        "@fusionconfig/api",
  "description": "The api for the fusionconfig project",
  "packageJson": {
    "dependencies":    {
      "@runbook/utils":              "0.3.15",
      "@laoban/utils":              "${properties.laobanVersion}",
      "@itsmworkbench/apiurlstore":"${properties.itsmVersion}",
      "@itsmworkbench/nodeurlstore": "${properties.itsmVersion}",
      "@itsmworkbench/cli":         "${properties.itsmVersion}",
      "@itsmworkbench/shellgit":   "${properties.itsmVersion}",
      "@itsmworkbench/jsyaml":   "${properties.itsmVersion}",
      "@itsmworkbench/koa":        "${properties.itsmVersion}"
    },
    "devDependencies": {}
  },
  "links":       [
    "@fusionconfig/alldomains",
    "@fusionconfig/config",
    "@fusionconfig/fileopsconfig"
  ],
  "guards":      {
    "compile": true,
    "publish": true,
    "test":    false
  }
}